	cxt.stroke();
	cxt.fillStyle = 'black';
    cxt.fill();
	
	cxt.beginPath();
	cxt.strokeStyle = 'black';
	cxt.lineWidth = 5;
    cxt.arc(385, 145, 5, 0, Math.PI, true);
	cxt.stroke();
	cxt.fillStyle = 'black';
    cxt.fill();
	
	cxt.beginPath();
	cxt.strokeStyle = 'black';
	cxt.lineWidth = 5;
    cxt.arc(385, 126, 5, Math.PI, 0, true);
	cxt.stroke();
	cxt.fillStyle = 'black';
    cxt.fill();
	


	cxt.beginPath();
    cxt.lineWidth = 3;
    cxt.strokeStyle = 'black';
    cxt.moveTo(340, 97);
    cxt.lineTo(350, 25);
    cxt.stroke();
	
