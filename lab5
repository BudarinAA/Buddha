<script>

function smile()
{
    var mycanvas = document.getElementById('smile');
    var cxt = mycanvas.getContext('2d');
    
	cxt.scale(1.3,1);
    cxt.beginPath();
	cxt.strokeStyle = 'black';
	cxt.lineWidth = 5;
    cxt.arc(300, 400, 100, 0, 2*Math.PI, true);
	cxt.stroke();
	cxt.fillStyle = '#FFFFE0';
    cxt.fill();
	cxt.scale(1/1.3,1);
	
	cxt.beginPath();
	cxt.strokeStyle = 'black';
	cxt.lineWidth = 5;
    cxt.arc(385, 230, 80, 0, 2*Math.PI, true);
	cxt.stroke();
	cxt.fillStyle = '#FFFFE0';
    cxt.fill();
	
	cxt.beginPath();
	cxt.strokeStyle = 'black';
	cxt.lineWidth = 5;
    cxt.arc(385, 120, 40, 0, 2*Math.PI, true);
	cxt.stroke();
	cxt.fillStyle = '#FFFFE0';
    cxt.fill();
	
	cxt.beginPath();
    cxt.lineWidth = 5;
    cxt.strokeStyle = 'black';
    cxt.moveTo(315, 190);
    cxt.lineTo(200, 250);
    cxt.stroke();

	cxt.beginPath();
    cxt.lineWidth = 5;
    cxt.strokeStyle = 'black';
    cxt.moveTo(455, 190);
    cxt.lineTo(550, 250);
    cxt.stroke();

	cxt.beginPath();
    cxt.lineWidth = 5;
