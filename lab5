    cxt.lineTo(562, 160);
    cxt.stroke();
	
	cxt.beginPath();
    cxt.lineWidth = 5;
    cxt.strokeStyle = 'black';
    cxt.moveTo(562, 160);
    cxt.lineTo(600, 80);
    cxt.stroke();
	
